{
  "name": "api.pcm-cmm.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-s copy:assets next:dev",
    "start": "run-s build:prod next:start",
    "build": "run-s copy:assets next:build",
    "build:prod": "run-s copy:assets next:build",
    "next:build": "next build",
    "next:export": "next export",
    "next:dev": "next dev",
    "next:start": "next start",
    "clean": "rimraf .next",
    "build-types": "tsc --noEmit --pretty",
    "lint": "next lint",
    "copy:assets": "copyfiles -u 1 \"./src/assets/**/*.+(png|svg|jpg|jpeg|webp|mp4|pdf|ico)\" public",
    "prepare": "husky install",
    "analize": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@sendgrid/client": "^7.6.1",
    "@sendgrid/mail": "^7.6.1",
    "airtable": "^0.11.4",
    "axios": "^0.26.1",
    "joi": "^17.6.0",
    "next": "^12.0.8",
    "nextjs-cors": "^2.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5",
    "webp-loader": "^0.6.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.2.5",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "^12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  }
}
